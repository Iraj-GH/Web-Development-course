fileRef = open(r'Web Development (AKR)\Python(S&S)\note.txt','a')

num = int(input("Enter a number: "))

content=f"Multipliation table of {num} :\n"
for i in range(1,101):
    content += f"{num} X {i} = {num*i} \n"

fileRef.write(content)

